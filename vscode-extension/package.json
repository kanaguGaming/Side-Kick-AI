{
  "name": "sidekick-ta",
  "displayName": "Sidekick: Autonomous Lab TA",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "activationEvents": [
    "*" 
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "sidekick-sidebar",
          "title": "Sidekick TA",
          "icon": "icon.png"
        }
      ]
    },
    "views": {
      "sidekick-sidebar": [
        {
          "type": "webview",
          "id": "sidekick.chatView",
          "name": "TA Chat Room"
        }
      ]
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "typescript": "^5.1.3"
  }
}